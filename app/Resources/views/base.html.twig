<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}Bootstrap 101 Template{% endblock %}</title>
        {% block stylesheets %}


            <link href="{{ asset('bundles/framework/css/bootstrap/navbar-fixed-top.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('bundles/framework/css/bootstrap/bootstrap.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('bundles/framework/css/bootstrap/bootstrap.min.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('bundles/framework/css/forms.css') }}" type="text/css" rel="stylesheet" />
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="{{ asset("https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js") }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/js/bootstrap.min.js') }}" type="text/javascript"></script>
        {% endblock %}
    </body>
</html>
